<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Member">

		<select id="member_read" resultType="member"> 	 
 	 		select * from golformemberNew where m_id=#{m_id} and m_state=1
 	 	</select>
 	 	
 	 	<insert id="member_in">
 		 	insert into golformemberNew (m_no,m_id,m_pw,m_phone,m_email,m_gender,m_date,m_state)
 		 	values (m_golf_seq.nextval,#{m_id},#{m_pw},#{m_phone},#{m_email},#{m_gender},sysdate,1)
 	 	</insert>
 	 	
 	 	<select id="id_check" resultType="member">
 	 		select * from golformemberNew where m_id=#{m_id}
 	 	</select>
 	 	
</mapper>
